<https://solidproject.org/about#features> schema:name "Features"@en;
    schema:description "\n              <ul xmlns=\"http://www.w3.org/1999/xhtml\" xmlns:doap=\"http://usefulinc.com/ns/doap#\" xmlns:foaf=\"http://xmlns.com/foaf/0.1/\" xmlns:schema=\"http://schema.org/\">\n                <li xmlns=\"http://www.w3.org/1999/xhtml\" xmlns:doap=\"http://usefulinc.com/ns/doap#\" xmlns:foaf=\"http://xmlns.com/foaf/0.1/\" xmlns:schema=\"http://schema.org/\">\n                  <p xmlns=\"http://www.w3.org/1999/xhtml\" xmlns:doap=\"http://usefulinc.com/ns/doap#\" xmlns:foaf=\"http://xmlns.com/foaf/0.1/\" xmlns:schema=\"http://schema.org/\"><strong xmlns=\"http://www.w3.org/1999/xhtml\" xmlns:doap=\"http://usefulinc.com/ns/doap#\" xmlns:foaf=\"http://xmlns.com/foaf/0.1/\" xmlns:schema=\"http://schema.org/\">Organizations Deploying Solid</strong></p>\n                  <p xmlns=\"http://www.w3.org/1999/xhtml\" xmlns:doap=\"http://usefulinc.com/ns/doap#\" xmlns:foaf=\"http://xmlns.com/foaf/0.1/\" xmlns:schema=\"http://schema.org/\">See organizations working to support the deployment of Solid technology across a range of\n                  sectors.</p>\n                  <a href=\"/for-organizations\" rel=\"rdfs:seeAlso\" xmlns=\"http://www.w3.org/1999/xhtml\" xmlns:doap=\"http://usefulinc.com/ns/doap#\" xmlns:foaf=\"http://xmlns.com/foaf/0.1/\" xmlns:schema=\"http://schema.org/\">See organizations</a>\n                </li>\n                <li xmlns=\"http://www.w3.org/1999/xhtml\" xmlns:doap=\"http://usefulinc.com/ns/doap#\" xmlns:foaf=\"http://xmlns.com/foaf/0.1/\" xmlns:schema=\"http://schema.org/\">\n                  <p xmlns=\"http://www.w3.org/1999/xhtml\" xmlns:doap=\"http://usefulinc.com/ns/doap#\" xmlns:foaf=\"http://xmlns.com/foaf/0.1/\" xmlns:schema=\"http://schema.org/\"><strong xmlns=\"http://www.w3.org/1999/xhtml\" xmlns:doap=\"http://usefulinc.com/ns/doap#\" xmlns:foaf=\"http://xmlns.com/foaf/0.1/\" xmlns:schema=\"http://schema.org/\">Develop a Solid application</strong></p>\n                  <p xmlns=\"http://www.w3.org/1999/xhtml\" xmlns:doap=\"http://usefulinc.com/ns/doap#\" xmlns:foaf=\"http://xmlns.com/foaf/0.1/\" xmlns:schema=\"http://schema.org/\">Find tutorials, repositories, libraries, and vocabularies.</p>\n                  <a href=\"/for-developers\" rel=\"rdfs:seeAlso\" xmlns=\"http://www.w3.org/1999/xhtml\" xmlns:doap=\"http://usefulinc.com/ns/doap#\" xmlns:foaf=\"http://xmlns.com/foaf/0.1/\" xmlns:schema=\"http://schema.org/\">Develop an application</a>\n                </li>\n                <li xmlns=\"http://www.w3.org/1999/xhtml\" xmlns:doap=\"http://usefulinc.com/ns/doap#\" xmlns:foaf=\"http://xmlns.com/foaf/0.1/\" xmlns:schema=\"http://schema.org/\">\n                  <p xmlns=\"http://www.w3.org/1999/xhtml\" xmlns:doap=\"http://usefulinc.com/ns/doap#\" xmlns:foaf=\"http://xmlns.com/foaf/0.1/\" xmlns:schema=\"http://schema.org/\"><strong xmlns=\"http://www.w3.org/1999/xhtml\" xmlns:doap=\"http://usefulinc.com/ns/doap#\" xmlns:foaf=\"http://xmlns.com/foaf/0.1/\" xmlns:schema=\"http://schema.org/\">Example applications</strong></p>\n                  <p xmlns=\"http://www.w3.org/1999/xhtml\" xmlns:doap=\"http://usefulinc.com/ns/doap#\" xmlns:foaf=\"http://xmlns.com/foaf/0.1/\" xmlns:schema=\"http://schema.org/\">Find examples of applications built with Solid.</p>\n                  <a href=\"https://forum.solidproject.org/c/applications/\" rel=\"rdfs:seeAlso\" xmlns=\"http://www.w3.org/1999/xhtml\" xmlns:doap=\"http://usefulinc.com/ns/doap#\" xmlns:foaf=\"http://xmlns.com/foaf/0.1/\" xmlns:schema=\"http://schema.org/\">See example applications</a>\n                </li>\n              </ul>\n            "^^rdf:HTML;
    rdfs:seeAlso <https://solidproject.org/for-organizations>, <https://solidproject.org/for-developers>, <https://forum.solidproject.org/c/applications/>.
